# Core
NODE_ENV=development
PORT=3000

# Monitored GitHub repositories (comma-separated URLs)
MONITORED_REPOS=https://github.com/openai/openai-node,https://github.com/langchain-ai/langchain

# AI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
GITHUB_TOKEN=

# Data
DATABASE_URL=sqlite:./data/clawstrack.json

# Notifications
NOTIFICATION_PROVIDER=console
NOTIFICATION_WEBHOOK_URL=

# Security/admin
ADMIN_API_KEY=change-me
SESSION_TTL_MS=604800000

# Worker scheduling
WORKER_INTERVAL_MS=900000
STARTUP_MONITORING_DELAY_MS=0
